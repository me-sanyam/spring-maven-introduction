<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="
        http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student1" class="com.SpringBeans.Student">
        <property name="rollNo" value="1"/>
        <property name="name" value="Sanyam Kapoor"/>
        <property name="email" value="sanyam.kapoor@codeoptimalsolutions.com"/>
        <property name="address" ref="address1"/>
    </bean>

    <bean id="address1" class="com.SpringBeans.Address">
        <property name="location" value="SCO-90, Ranjit Avenue"/>
        <property name="city" value="Amritsar"/>
        <property name="state" value="Punjab"/>
        <property name="zipcode" value="143001"/>
    </bean>

</beans>